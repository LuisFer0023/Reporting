Private Sub TextBox2_Exit(ByVal Cancel As MSForms.ReturnBoolean)
If Len(TextBox2) = 9 Then
TextBox2 = 0 & TextBox2
CommandButton2.Enabled = True
Exit Sub
Else
End If
End Sub

